<p id="notice"><%= notice %></p>

<h1>Dishes Products</h1>

<table>
  <thead>
    <tr>
      <th>Plate</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dishes_products.each do |dishes_product| %>
      <tr>
        <td><%= dishes_product.plate %></td>
        <td><%= dishes_product.product %></td>
        <td><%= link_to 'Show', dishes_product %></td>
        <td><%= link_to 'Edit', edit_dishes_product_path(dishes_product) %></td>
        <td><%= link_to 'Destroy', dishes_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dishes Product', new_dishes_product_path %>
